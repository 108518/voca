{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "src/"
  },
  "plugins": [
    "jsdoc/plugins/category"
  ],
  "opts": {
    "template": "jsdoc/template",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}