{
  "name": "voca",
  "version": "0.0.1",
  "description": "The ultimate JavaScript string library",
  "main": "dist/voca.js",
  "jsnext:main": "dist/voca.es6.js",
  "scripts": {
    "eslint": "eslint . --ext .js",
    "build": "rollup -c rollup.config.js",
    "build-test": "rollup -c rollup.test.config.js",
    "test": "mocha test/index.js --reporter dot",
    "coverage": "istanbul cover _mocha -- test/index.js --reporter spec",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "verify": "npm run eslint && npm run coverage",
    "prepublish": "npm test",
    "jsdoc": "jsdoc --configure .jsdoc.json && opn docs/index.html",
    "deploy": "scp -r docs/* rainishere:/home/rainishere/webapps/voca_docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panzerdp/voca.git"
  },
  "author": "Dmitri Pavlutin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/panzerdp/voca/issues"
  },
  "homepage": "https://github.com/panzerdp/voca#readme",
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-plugin-external-helpers": "6.8.0",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.13.2",
    "babel-register": "6.11.6",
    "babelrc-rollup": "3.0.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "eslint": "2.13.1",
    "ghooks": "1.3.2",
    "istanbul": "1.1.0-alpha.1",
    "jsdoc": "3.4.0",
    "mocha": "3.0.2",
    "opn-cli": "3.1.0",
    "rewire": "2.5.2",
    "rollup": "0.34.7",
    "rollup-plugin-babel": "2.6.1",
    "semver-regex": "1.0.0",
    "source-map-support": "0.4.2"
  },
  "dependencies": {},
  "config": {
    "ghooks": {
      "pre-commit": "npm run eslint && npm run test"
    }
  }
}
